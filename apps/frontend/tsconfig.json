{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "moduleResolution": "node",
    "resolveJsonModule": true,




    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "sourceMap": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
        "@core": ["../../libs/core"],
        "@frontend/api-sdk": ["../../libs/frontend/api-sdk/gen/index.ts"]
    },
    "noImplicitAny": false,

      "types": ["@intlify/unplugin-vue-i18n/messages"]
  },
  "files": [],
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "extends": "../../tsconfig.base.json"
}
